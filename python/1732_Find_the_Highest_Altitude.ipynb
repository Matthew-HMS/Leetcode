    "class Solution:\n",
    "    def largestAltitude(self, gain: List[int]) -> int:\n",
    "        max_alt = alt = 0\n",
    "        for i in range(len(gain)):\n",
    "            alt += gain[i]\n",
    "            max_alt = max(max_alt, alt)\n",
    "        \n",
    "        return max_alt"
   ]
