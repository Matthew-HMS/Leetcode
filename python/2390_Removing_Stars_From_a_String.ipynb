   "source": [
    "class Solution:\n",
    "    def removeStars(self, s: str) -> str:\n",
    "        stack = []\n",
    "        for c in s:\n",
    "            if c != '*':\n",
    "                stack.append(c)\n",
    "            else:\n",
    "                stack.pop()\n",
    "        \n",
    "        return ''.join(stack)"
   ]
  }
